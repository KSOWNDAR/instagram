<div class = "icons">
<%= form_with url:search_index_path, method: :get, local: true, class: 'form-inline search-form' do |form| %>
  <%= form.text_field :query, value: params[:query], search_id: 1, class: 'form-control mr-sm-2', placeholder: '#search' %>
<% end %>

<li class = "nav-item active">
  <%= link_to "Home", dashboard_path, class: "nav-link" %>
</li>
<li class = "nav-item">
  <%= link_to "New Post", new_post_path, class: "nav-link" %>
</li>
<li class = "nav-item">
  <%= link_to "My Profile", profile_path(current_account.username), class: "nav-link" %>
</li>
<li class = "nav-item">
  <%= link_to "Sign Out", destroy_account_session_path, method: :delete, class: "nav-link" %>
</li>
</div>
  